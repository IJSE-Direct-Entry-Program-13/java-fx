<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="173.0" prefWidth="279.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep13.fx.controller.MainSceneController">
   <children>
      <TextField fx:id="txtCountry" layoutX="22.0" layoutY="23.0" />
      <ComboBox fx:id="cmbCountry" layoutX="22.0" layoutY="65.0" prefHeight="26.0" prefWidth="162.0" />
      <Button fx:id="btnAdd" defaultButton="true" layoutX="198.0" layoutY="23.0" mnemonicParsing="false" onAction="#btnAddOnAction" prefHeight="26.0" prefWidth="63.0" text="ADD" />
      <Label fx:id="lblSelectedCountry" layoutX="22.0" layoutY="102.0" text="No Country Selected">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Button fx:id="btnClearSelection" layoutX="22.0" layoutY="132.0" mnemonicParsing="false" onAction="#btnClearSelectionOnAction" text="CLEAR SELECTION" />
   </children>
</AnchorPane>
