         </rowConstraints>
         <children>
            <Label text="RED" />
            <Label text="GREEN" GridPane.columnIndex="1" />
            <Label text="BLUE" GridPane.columnIndex="2" />
            <TextField fx:id="txtRed" alignment="CENTER" editable="false" prefHeight="26.0" prefWidth="65.0" GridPane.rowIndex="1" />
            <TextField fx:id="txtGreen" alignment="CENTER" editable="false" prefHeight="26.0" prefWidth="65.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="txtBlue" alignment="CENTER" editable="false" prefHeight="26.0" prefWidth="65.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="btnChangeColor" layoutX="228.0" layoutY="218.0" mnemonicParsing="false" onAction="#btnChangeColorOnAction" text="CHANGE COLOR" />
   </children>
</AnchorPane>
