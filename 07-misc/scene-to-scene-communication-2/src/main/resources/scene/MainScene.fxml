<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" style="-fx-font-size: 16px;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep13.fx.controller.MainSceneController">
   <children>
      <GridPane hgap="5.0" layoutX="198.0" layoutY="151.0" vgap="5.0" AnchorPane.leftAnchor="198.0" AnchorPane.rightAnchor="197.0">
